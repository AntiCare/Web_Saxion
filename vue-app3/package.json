{
  "name": "vuejs-crud-application",
  "description": "A demo application to show how to build a crud application with VueJS",
  "author": "Paulund",
  "private": true,
  "scripts": {
    "front": "cross-env NODE_ENV=development webpack serve --open --inline --hot",
    "back": "nodemon server",
    "dual": "concurrently --kill-others \"node server\" \"cross-env NODE_ENV=development webpack serve --open --inline --hot\"",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "serve": "webpack serve --mode=development"
  },
  "type": "module",
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "sqlite": "^4.0.22",
    "sqlite3": "^5.0.2",
    "vue": "^2.6.12"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@vue/cli": "^4.5.13",
    "@vue/cli-plugin-babel": "^4.5.13",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-preset-es2015": "^6.0.0",
    "concurrently": "^6.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.5",
    "file-loader": "^6.2.0",
    "nodemon": "^2.0.7",
    "style-loader": "^2.0.0",
    "vue-loader": "^15.9.2",
    "vue-resource": "^1.0.3",
    "vue-router": "^3.5.1",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  }
}
